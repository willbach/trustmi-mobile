{
  "name": "the-pond",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-decorators": "^7.1.6",
    "@types/jest": "^20.0.8",
    "@types/react": "^16.0.5",
    "@types/react-native": "^0.47.7",
    "@types/react-native-vector-icons": "^4.3.1",
    "@types/react-navigation": "^2.0",
    "assert": "^1.4.1",
    "babel-eslint": "^8.1.0",
    "babel-jest": "^22.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-react-native": "^3.0.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "concurrently": "3.5.1",
    "eslint": "4.14.0",
    "eslint-plugin-flowtype": "2.35.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-prettier": "2.2.0",
    "eslint-plugin-react": "7.3.0",
    "eslint-plugin-react-native": "3.1.0",
    "husky": "0.14.3",
    "jest-expo": "^24.0.0",
    "react-devtools-core": "3.4.2",
    "react-test-renderer": "16.2.0",
    "rimraf": "^2.6.2",
    "ts-jest": "22.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "url": "^0.11.0"
  },
  "scripts": {
    "typescript": "node_modules/.bin/tsc -w --skipLibCheck",
    "start": "react-native start",
    "jest": "node node_modules/jest/bin/jest.js",
    "watch": "npm run build -- -w",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "tsc": "tsc",
    "build": "npm run clean && npm run tsc --",
    "lint": "tslint src/**/*.ts",
    "test": "yarn jest && tslint src/**/*.tsx && eslint .",
    "postinstall": "node_modules/.bin/rn-nodeify --install stream,crypto --hack",
    "setup:env": "node scripts/env.js"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|lottie-react-native|expo|react-native-maps|react-native-svg|react-native-branch|native-base-shoutem-theme|react-native-easy-grid|react-native-drawer|react-native-vector-icons|react-native-keyboard-aware-scroll-view|react-navigation|native-base|@expo|react-native-scrollable-tab-view)/)"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@types/node": "^10.12.9",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "bcryptjs": "^2.4.3",
    "bitcore-mnemonic": "^1.5.0",
    "crypto-js": "^3.1.9-1",
    "ethereumjs-util": "^5.2.0",
    "events": "^3.0.0",
    "lodash": "4.17.4",
    "mobx": "3.4.1",
    "mobx-react": "4.3.5",
    "moment": "^2.22.2",
    "native-base": "2.7.0",
    "react": "^16.6.1",
    "react-native": "^0.57.1",
    "react-native-blur": "^3.2.2",
    "react-native-calendars": "^1.21.0",
    "react-native-crypto": "^2.1.2",
    "react-native-image-crop-picker": "^0.21.3",
    "react-native-paper": "^2.1.3",
    "react-native-randombytes": "^3.5.1",
    "react-native-splash-screen": "^3.1.1",
    "react-navigation": "^2.0",
    "react-navigation-material-bottom-tabs": "^0.4.0",
    "readable-stream": "^1.0.33",
    "rn-nodeify": "^10.0.0",
    "stream": "^0.0.2",
    "stream-browserify": "^2.0.1",
    "underscore": "^1.9.1",
    "vm": "^0.1.0"
  },
  "react-native": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  },
  "browser": {
    "crypto": "react-native-crypto",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "stream": "stream-browserify"
  }
}
